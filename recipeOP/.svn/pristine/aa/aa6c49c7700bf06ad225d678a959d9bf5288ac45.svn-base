package com.onethefull.recipe.vo;


public class RecipeIngrVO {
	String ingredientId;
	String ingredientName;
	String ingredientQnt;
	
	public RecipeIngrVO() {
		super();
		// TODO Auto-generated constructor stub
	}
	public RecipeIngrVO(String ingredientId, String ingredientName, String ingredientQnt) {
		super();
		this.ingredientId = ingredientId;
		this.ingredientName = ingredientName;
		this.ingredientQnt = ingredientQnt;
	}
	public String getIngredientQnt() {
		return ingredientQnt;
	}
	public void setIngredientQnt(String ingredientQnt) {
		this.ingredientQnt = ingredientQnt;
	}
	public String getIngredientId() {
		return ingredientId;
	}
	public void setIngredientId(String ingredientId) {
		this.ingredientId = ingredientId;
	}
	public String getIngredientName() {
		return ingredientName;
	}
	public void setIngredientName(String ingredientName) {
		this.ingredientName = ingredientName;
	}
}
